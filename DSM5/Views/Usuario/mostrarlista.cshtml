
@{
    ViewBag.Title = "mostrarlista";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="container-fluid bg-2 text-center">
    <h2 class="margin">Lista</h2>

    <p>
        @Html.Partial("_Pelicula", (IEnumerable<DSM5.Models.Pelicula>)ViewBag.peli)
        @Html.Partial("_Serie", (IEnumerable<DSM5.Models.Serie>)ViewBag.serie)
    </p>

    <div class="form-group">
        <div class="col-md-8">
            @Html.ActionLink("Volver", "Details", "Usuario", new { id = (string)@Session["correo"] }, null)
        </div>
    </div>


<h2>@Session["lista"] </h2>

</div>


