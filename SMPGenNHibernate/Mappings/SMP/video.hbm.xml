<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="SMPGenNHibernate"
		                   namespace="SMPGenNHibernate.EN.SMP">
		    
		    
			
				<class name="VideoEN" table="video"> 
				  	
						<id name="Id" column="idVideo" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Valoracion" > 
						<column name="valoracion"  not-null="true"/>
					</property>
					<property name="Nombre" type="string"> 
						<column name="nombre"  not-null="true"/>
					</property>
					<property name="Imagen" type="string"> 
						<column name="imagen"  not-null="true"/>
					</property>
					<property name="Descriplarga" type="string"> 
						<column name="descriplarga" length="60000" not-null="true"/>
					</property>
					<property name="Descripcion" type="string"> 
						<column name="descripcion" length="60000" not-null="true"/>
					</property>
					<property name="Genero" type="string"> 
						<column name="genero"  not-null="true"/>
					</property>
					<property name="Anyo" type="date"> 
						<column name="anyo"  not-null="true"/>
					</property>
					<property name="Imagran" type="string"> 
						<column name="imagran"  not-null="true"/>
					</property>
			
			
			
			
			
			
			
			
			
			
					
					
					
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="VideoENReadAllHQL">from VideoEN</query>
		</hibernate-mapping>
	